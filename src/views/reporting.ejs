<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sidebar With Bootstrap</title>
    <link href="https://cdn.lineicons.com/4.0/lineicons.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/admin.css">
</head>

<body>
    <div class="wrapper">
        <aside id="sidebar">
            <div class="d-flex">
                <button class="toggle-btn" type="button">
                    <i class="lni lni-grid-alt"></i>
                </button>
                <div class="sidebar-logo">
                    <img class="img-fluid" src="/images/logo.png" alt="">
                </div>
            </div>
            <ul class="sidebar-nav">
                <li class="sidebar-item">
                    <a href="/admin" class="sidebar-link">
                        <i class="lni lni-protection"></i>
                        <span>Hall Management</span>
                    </a>
                </li>
                <li class="sidebar-item">
                    <a href="/resource" class="sidebar-link">
                        <i class="lni lni-agenda"></i>
                        <span>Ressource Management</span>
                    </a>
                </li>
                <li class="sidebar-item">
                    <a href="/users" class="sidebar-link " >
                        <i class="lni lni-user"></i>
                        <span>User Management</span>
                    </a>
                </li>
                <li class="sidebar-item">
                    <a href="/reporting" class="sidebar-link  ">
                        <i class="lni lni-layout"></i>
                        <span>Reporting</span>
                    </a>
                    
                </li>
            </ul>
            <div class="sidebar-footer">
                <a href="/login" class="sidebar-link">
                    <i class="lni lni-exit"></i>
                    <span>Logout</span>
                </a>
            </div>
        </aside>
        <div class="main">
            <nav class="">
               
            </nav>
            <main class="content px-3 py-4">
                <div class="container-fluid">
                    <div class="mb-3">
                        
                        <div class="add-hall-btn-container">
                            <h3 class="fw-bold fs-4 mb-3 title">Reporting</h3>
                            
                        </div>           
            <main>
                <table class="table table-striped">
                    <thead class="thead-dark">
                      <tr>
                        
                        <th scope="col">hall name</th>
                        <th scope="col">start date</th>
                        <th scope="col">end date</th>
                        <th scope="col">Resources</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% bookingResources.forEach(bookingResource => { %>
                            <tr>
                                <td><%= bookingResource.booking && bookingResource.booking.Hall && bookingResource.booking.Hall.name %></td>
                                <td><%= bookingResource.booking && bookingResource.booking.startDate %></td>
                                <td><%= bookingResource.booking && bookingResource.booking.endDate %></td>
                                <td><%= bookingResource.resource && bookingResource.resource.name %></td>
                            </tr>
                        <% }) %>
                        
                    </tbody>
                  </table>
            </main>
            

  
  
            <footer class="footer">
              
            </footer>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
    <script src="/js/sidebar.js"></script>
</body>
</html>